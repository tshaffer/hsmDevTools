{
  "name": "@brightsign/bs-ui-template",
  "version": "0.1.0",
  "private": true,
  "author": "BrightSign LLC",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@git.brightsign.biz:bs-ui-template"
  },
  "typings": "./index.d.ts",
  "main": "./dist/electron/main.js",
  "browser": {
    "./dist/electron/main.js": "./dist/electron/main.browser.js"
  },
  "scripts": {
    "start-electron": "cross-env PLATFORM=electron node scripts/start.js",
    "start-browser": "cross-env PLATFORM=browser node scripts/start.js",
    "build-dev-electron-main": "webpack --config ./config/webpack.config.main.js",
    "package-link": "npm link typescript && npm link webpack",
    "package-install": "npm install --only=production",
    "build-browser": "cross-env PLATFORM=browser node scripts/build.js",
    "build-electron": "cross-env PLATFORM=electron node scripts/build.js",
    "build-standalone": "cross-env PLATFORM=standalone node scripts/build.js",
    "package-submodule": "node scripts/clean.js && npm run build-standalone && npm run build-browser && npm run build-electron",
    "test": "node scripts/test.js --env=jsdom",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/utils": "https://registry.npmjs.org/@material-ui/utils/-/utils-4.7.1.tgz",
    "csstips": "^0.2.2",
    "csx": "^8.5.0",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "^4.0.3",
    "isomorphic-fetch-extended": "2.0.2",
    "isomorphic-path": "0.0.5",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "typestyle": "^1.5.1"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.6",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^22.0.0",
    "@types/lodash": "^4.14.92",
    "@types/node": "^8.5.2",
    "@types/react": "^16.8.15",
    "@types/react-addons-test-utils": "0.14.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-router": "3.0.19",
    "autoprefixer": "7.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "0.28.4",
    "csx": "^8.5.0",
    "devtron": "^1.4.0",
    "dotenv": "4.0.0",
    "dts-bundle": "^0.7.3",
    "electron": "1.7.9",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "normalize.css": "^5.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "raf": "^3.4.0",
    "react-dev-utils": "^4.0.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^3.6.4",
    "typestyle": "^1.5.1",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/adapter.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  }
}
